            	<%
            	if(stdRespList != null && stdRespList.size() > 0){
	            	int rowNumb = (stdRespList.size() / 4); 
	            	int divide = stdRespList.size() % 4;
	           		if((rowNumb == 0 || divide > 0)){
	           			rowNumb = rowNumb + 1;
	           		}
	           		
	           		int end = 0;
	           		int start = 0;
	            	for(int i = 0; i < rowNumb; i++){
	            		if((i == (rowNumb - 1)) && divide > 0){
	            			end = ((i * 4) + (stdRespList.size() % 4));
	            		}else{
	            			end = ((i + 1) * 4);
	            		}
	            		
	            		start = (i * 4);
            	%>
                <div class="row">
                	<%
                		for(int j = start ; j < end;j++){
                	%>
                    <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
                        <div class="student-inner-std res-mg-b-30">
                            <div class="student-img">
                                <img src="data:image/jpeg;base64,<%=stdRespList.get(j).getImageCode()%>" alt="" />
                            </div>
                            <div class="student-dtl">
                                <a href="./EditStudentServlet?mode=edit&studentID=<%=stdRespList.get(j).getStudentID()%>"><h2><%=stdRespList.get(j).getFullName()%></h2></a>
                                <p class="dp"><%=stdRespList.get(j).getDepartment()%></p>
                                <p class="dp-ag"><b>Age:</b> <%=stdRespList.get(j).getAge()%> Years</p>
                                <p class="dp-ag"><a href="./EditStudentServlet?mode=delete&studentID=<%=stdRespList.get(j).getStudentID()%>"><button type="button" class="btn btn-custon-rounded-three btn-danger"><i class="fa fa-times edu-danger-error" aria-hidden="true"></i> Delete</button></a></p>
                            </div>
                        </div>
                    </div>
                    <%
                    	}
                    %>
                </div>
